<?xml version='1.0' encoding='utf-8'?>
<widget id="pe.picnic.escuelavr" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Escuela VR</name>
    <description>
        Escuela VR
    </description>
    <author email="jonatan@picnic.pe" href="http://picnic.pe">
        Picnic Lab
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="phonegap-version" value="cli-5.2.0" />
    <plugin spec="https://github.com/FranciscoG/cordova-vr-player.git" source="git" />
</widget>
